    "runtime-version":"3.30",
    "sdk":"org.gnome.Sdk",
    "sdk-extensions":[
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command":"sharexin",
    "build-options":{
        "append-path":"/usr/lib/sdk/rust-stable/bin",
        "build-args":[
            "--share=network"
        ],
        "env":{
            "CARGO_HOME":"/run/build/sharexin/cargo",
            "DEBUG":"true"
        }
    },
    "finish-args":[
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=xdg-pictures",
        "--env=TMPDIR=/var/tmp",
        "--share=network",
        "--share=ipc"
    ],
    "modules":[
        {
            "name":"sharexin",
            "sources":[
                {
                    "type":"git",
                    "url":"https://github.com/ShareXin/ShareXin"
                }
            ],
            "subdir":"rust",
            "buildsystem":"simple",
            "build-args":[
                "--share=network"
            ],
            "build-commands":[
                "cargo build --release",
                "install -D target/release/sharexin /app/bin/sharexin"
            ]
        },
